{
  "name": "vorpal-wechaty-hackernews",
  "version": "1.0.0",
  "description": "Vorpal with Wechaty: Hackernews",
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "gotty": "bash -x install-gotty.sh",
